{
  "name": "react-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && npx serve@latest out",
    "git-hooks-install": "husky install",
    "storybook-start": "start-storybook -p 6006",
    "storybook-build": "build-storybook",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "cypress run --headless --browser chrome",
    "test:e2e-local": "cypress open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "next build && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:mobile": "next build && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve@latest out\"",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.8",
    "@sentry/node": "^9.9.0",
    "@sentry/react": "^9.9.0",
    "@testing-library/dom": "^10.4.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "i18next": "^24.2.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "next": "^15.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.1",
    "reflect-metadata": "^0.2.2",
    "require-from-string": "^2.0.2",
    "storybook": "^8.6.9",
    "terser-webpack-plugin": "^5.3.14",
    "tsyringe": "^4.8.0",
    "web-vitals": "^4.2.4",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@lhci/cli": "^0.14.0",
    "@microsoft/api-extractor": "^7.52.1",
    "@next/eslint-plugin-next": "^15.2.3",
    "@storybook/addon-actions": "^8.6.9",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/builder-webpack5": "^8.6.9",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.6.9",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-testing-library": "^6.5.0",
    "husky": "^9.1.7",
    "markdownlint-cli": "^0.44.0",
    "memlab": "^1.1.57",
    "prettier": "^3.5.3",
    "semver": "^7.7.1",
    "ts-jest": "^29.3.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
